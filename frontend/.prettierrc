{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{ "files": "*.svelte", "options": { "parser": "svelte" } },
		{ "files": "*.ts", "options": { "parser": "typescript" } },
		{ "files": "*.js", "options": { "parser": "babel" } },
		{ "files": "*.css", "options": { "parser": "css" } }
	]
}
